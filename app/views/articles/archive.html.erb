<div class="articlearchive column">
  <% @article.each do |article| %>
  <div class = "article_block">
  <h1>
   <%= h article.title %>
  </h1>
  <h2>
    Рубрика: <%= h article.rubric %>
  </h2>
  <div class="article_content">
    <%= article.content %>
  </div>
  <h2 class="article">
    Автор: <%= h article.autor %>
  </h2>
  </div>
  <% end %>
</div>
